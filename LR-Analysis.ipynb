   "metadata": {},
   "outputs": [],
   "source": [
    "data = []\n",
    "base = \"data\"\n",
    "for tourn in os.listdir(base):\n",
    "    for game in os.listdir(os.path.join(base, tourn)):\n",
    "        data.append(pd.read_csv(os.path.join(base, tourn, game)))\n",
    "pd.concat(data).to_csv(\"all_data.csv\")"
   ]
